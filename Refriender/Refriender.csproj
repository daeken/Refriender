<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net5.0</TargetFramework>
        <PackAsTool>true</PackAsTool>
        <ToolCommandName>refriender</ToolCommandName>
        <PackageOutputPath>./nupkg</PackageOutputPath>
        <PackageVersion>1.1.0</PackageVersion>
        <AssemblyVersion>1.1.0.0</AssemblyVersion>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Description>Refriender is a tool for reverse-engineering and analyzing archive file formats.</Description>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\RefrienderCore\RefrienderCore.csproj" />
    </ItemGroup>

</Project>
